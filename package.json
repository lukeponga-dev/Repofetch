{
  "name": "repofetch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./scripts/fetchRepos.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "echo \"No build step configured\"",
    "start:prod": "node dist/index.js",
    "clean": "rimraf output/ dist/ coverage/ || true",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "test": "echo \"No tests configured\" && exit 0",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^19.0.7",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "octokit": "^5.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.4.2",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.8.0",
    "rimraf": "^5.0.0",
    "smee-client": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
